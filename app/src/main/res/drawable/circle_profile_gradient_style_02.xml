<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="100dp"
    android:height="100dp"
    android:viewportWidth="100"
    android:viewportHeight="100">

    <group>

        <clip-path android:pathData="M0 0H100V100H0V0Z" />

        <group
            android:pivotX="50"
            android:pivotY="50"
            android:rotation="90"
            android:scaleX="2"
            android:scaleY="2"
            android:translateX="1"
            android:translateY="0">

            <path android:pathData="M0 0V100H100V0">

                <aapt:attr name="android:fillColor">

                    <gradient
                        android:endX="75"
                        android:endY="50"
                        android:startX="25"
                        android:startY="50"
                        android:type="linear">

                        <item
                            android:color="#4FA5F4"
                            android:offset="0" />

                        <item
                            android:color="#3EA0B6"
                            android:offset="0.401" />

                        <item
                            android:color="#450DBC"
                            android:offset="1" />

                    </gradient>

                </aapt:attr>

            </path>

        </group>

    </group>

</vector>

